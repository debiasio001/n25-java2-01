package JOptionPane;

import javax.swing.JOptionPane;

public class Exercicio01 {

	public static void main(String[] args) {
		JOptionPane.showConfirmDialog(null, "olá, participante, e bem-vindo ao progama "+ null + "de namoro favorito da américa!");
		JOptionPane.showMessageDialog(null,
                "olá, participante, e bem-vindo ao progama de namoro favorito da américa!",
                "Bem-Vindo, participante!",
                JOptionPane.INFORMATION_MESSAGE);
		
		
	}

}
